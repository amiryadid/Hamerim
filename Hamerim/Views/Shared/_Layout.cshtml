<!DOCTYPE html>
<html dir="rtl">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="mynavbar">
        <a href="/Home/Index" class="mytab">
        <img class="logoImg" src="~/Content/logo.png"/>
        </a>
        <a href="/Home/About" class="mytab">הזמן מקום</a>
        <a href="/Home/Contact" class="mytab">ההזמנות שלי</a>
        <a href="/Home/About" class="mytab" id="managerTab">מנהל</a>
    </div>
    <div>
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
